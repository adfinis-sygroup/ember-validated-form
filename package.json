{
  "name": "ember-validated-form",
  "version": "3.0.2",
  "description": "Easily create forms with client-side validations",
  "keywords": [
    "ember-addon",
    "forms",
    "form validation",
    "validation messages"
  ],
  "repository": "https://github.com/adfinis-sygroup/ember-validated-form",
  "license": "MIT",
  "author": "Adfinis SyGroup AG <info@adfinis-sygroup.ch>",
  "directories": {
    "test": "tests"
  },
  "homepage": "https://adfinis-sygroup.github.io/ember-validated-form",
  "bugs": {
    "url": "https://github.com/adfinis-sygroup/ember-validated-form/issues"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "ember-auto-import": "^1.10.1",
    "ember-cli-babel": "^7.23.1",
    "ember-cli-htmlbars": "^5.4.0",
    "ember-one-way-select": "^4.0.0",
    "ember-truth-helpers": "^3.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@adfinis-sygroup/eslint-config": "1.3.2",
    "@adfinis-sygroup/semantic-release-config": "3.1.0",
    "@babel/core": "7.12.16",
    "@ember/optional-features": "2.0.0",
    "@fortawesome/ember-fontawesome": "0.2.2",
    "@fortawesome/free-solid-svg-icons": "5.15.2",
    "@glimmer/component": "1.0.3",
    "@glimmer/tracking": "1.0.3",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-changeset": "3.12.0",
    "ember-changeset-validations": "3.13.0",
    "ember-cli": "3.25.0",
    "ember-cli-addon-docs": "1.0.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-deploy": "1.0.2",
    "ember-cli-deploy-build": "2.0.0",
    "ember-cli-deploy-git": "1.3.4",
    "ember-cli-deploy-git-ci": "1.0.1",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.1",
    "ember-cli-test-loader": "3.0.0",
    "ember-concurrency": "1.3.0",
    "ember-data": "3.25.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-power-select": "4.1.2",
    "ember-qunit": "4.6.0",
    "ember-resolver": "8.0.2",
    "ember-source": "3.25.1",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "2.20.0",
    "ember-try": "1.4.0",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-ember": "9.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "qunit-dom": "1.6.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://adfinis-sygroup.github.io/ember-validated-form"
  },
  "release": {
    "extends": "@adfinis-sygroup/semantic-release-config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
